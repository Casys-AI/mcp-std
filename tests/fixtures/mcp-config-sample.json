{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "protocol": "stdio"
    },
    "github": {
      "command": "mcp-server-github",
      "args": ["--token", "ghp_test"],
      "env": {
        "GITHUB_TOKEN": "ghp_test"
      },
      "protocol": "stdio"
    },
    "memory": {
      "command": "mcp-server-memory",
      "protocol": "stdio"
    }
  }
}
